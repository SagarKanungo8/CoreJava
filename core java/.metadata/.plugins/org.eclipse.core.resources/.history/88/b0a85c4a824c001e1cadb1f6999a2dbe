package in.com.IO;


import java.io.File;
import java.io.FileReader;
import java.io.IOException;

public class FileReader1 {
	public static void main(String[] args) throws IOException {
		
		FileReader reader = new FileReader("C:\\Users\\Pc\\Desktop\\IOSFolder\\New.txt");
		
		int ch = reader.read();
		while(ch!=-1) {
			System.out.println((char)ch);
		ch = reader.read();
				
		}
		reader.close();
	}

}
