package in.com.raysOOP;

public class TestShape {

	public static void main(String[] args) {
	Shape s = new Shape(10,"Red");
	//System.out.println(s.getColor());
	//System.out.println(s.getBorderWidth());

	s.setColor("Blue");
	s.setBorderWidth(100);
	System.out.println(s.getColor());
	System.out.println(s.getBorderWidth());
	}

}
